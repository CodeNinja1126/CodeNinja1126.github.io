            <span class="k">if</span> <span class="n">customer</span><span class="p">[</span><span class="mi">10</span><span class="p">].</span><span class="n">upper</span><span class="p">()</span> <span class="o">==</span> <span class="s">"USA"</span><span class="p">:</span> 
            <span class="c1">#customer ë°ì´í„°ì˜ offset 10ë²ˆì§¸ ê°’
</span>                <span class="n">customer_USA_only_list</span><span class="p">.</span><span class="n">append</span><span class="p">(</span><span class="n">customer</span><span class="p">)</span> 
                <span class="c1">#ì¦‰ country í•„ë“œê°€ â€œUSAâ€ ê²ƒë§Œ
</span>        <span class="n">line_counter</span><span class="o">+=</span><span class="mi">1</span> <span class="c1">#customer_USA_only_listì— ì €ì¥
</span>        
