<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="mi">0</span><span class="p">,</span><span class="mi">10</span><span class="p">):</span>
    <span class="k">print</span> <span class="p">(</span><span class="s">"Data :</span><span class="se">\t\t</span><span class="s">"</span><span class="p">,</span><span class="n">customer_USA_only_list</span><span class="p">[</span><span class="n">i</span><span class="p">])</span>
<span class="k">print</span> <span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">customer_USA_only_list</span><span class="p">))</span>

